javascript: delay=prompt("Enter delay (1). Any faster (less) than ~2 and typingclub may not register"); /*Educational purposes*/ var rawElements = document.getElementsByClassName("token_unit");var listElements = Array.from(rawElements); var letterList = []; listElements.forEach(item => letterList.push(item.innerHTML.replace(/<\/?[^>]+(>|$)/g, ""))); var strippedLetterList = letterList.map(function(x){return x.trim().replace(/&nbsp;/g, " ");}); for (let i = 0; i < strippedLetterList.length; i++) {setTimeout(function(){window.core.record_keydown_time(strippedLetterList[i])}, delay*i)};
